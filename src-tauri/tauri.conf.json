{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "yarn run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "only-refs",
  "mainBinaryName": "only-refs",
  "version": "0.0.6",
  "identifier": "com.onlyref",
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEUxQzlFNThGQkQzRTlFRkQKUldUOW5qNjlqK1hKNGRyL3FCRE50d256TUhBWE9JNkxQWFBhbEYva0NZK0ZkS24raGhFdXU2OW8K",
      "endpoints": [
        "https://tauri-update-server.freedominwork.workers.dev/v1/{{target}}/{{arch}}/{{current_version}}"
      ]
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "decorations": false,
        "title": "only-refs",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": [
          "$APPDATA/**"
        ],
        "enable": true
      },
      "csp": "default-src: 'self'; media-src 'self' asset: https://asset.localhost; connect-src ipc: http://ipc.localhost"
    }
  }
}